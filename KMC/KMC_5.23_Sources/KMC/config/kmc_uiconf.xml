	<upload>
		<!-- number of concurrent uploads -->
		<concurrent>2</concurrent>
		<!-- max size of single file in MB -->
		<maxSize>2047</maxSize>
	</upload>
	<modules>
		<module id="dashboard" displayName="Dashboard" subtab="" uiconf="@@DashboardUIConfID@@"
