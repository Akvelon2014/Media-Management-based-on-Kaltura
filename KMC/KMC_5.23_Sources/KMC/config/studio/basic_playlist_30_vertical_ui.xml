          <Button id="captureThumbBtnStartScreen" kClick="sendNotification('captureThumbnail')" buttonType="onScreenButton" minWidth="60" labelPlacement="top" label="Thumb" upIcon="thumbIcon" styleName="onScreenBtn"/>
