          <Button id="custom1BtnEndScreen" kClick="jsCall('customFunc1', mediaProxy.entry.id )" label="Custom 1" buttonType="onScreenButton" minWidth="60" labelPlacement="top" styleName="onScreenBtn" icon="generalIcon" upIcon="generalIcon" downIcon="generalIcon"/>
          <Button id="custom2BtnEndScreen" label="Custom 2" kClick="jsCall('customFunc2', mediaProxy.entry.id )" buttonType="onScreenButton" minWidth="60" labelPlacement="top" styleName="onScreenBtn" icon="generalIcon" upIcon="generalIcon" downIcon="generalIcon"/>
          <Button id="custom3BtnEndScreen" label="Custom 3" kClick="jsCall('customFunc3', mediaProxy.entry.id )" buttonType="onScreenButton" minWidth="60" labelPlacement="top" styleName="onScreenBtn" icon="generalIcon" upIcon="generalIcon" downIcon="generalIcon"/>
          <Button id="custom4BtnEndScreen" kClick="jsCall('customFunc4', mediaProxy.entry.id )" label="Custom 4" buttonType="onScreenButton" minWidth="60" labelPlacement="top" styleName="onScreenBtn" icon="generalIcon" upIcon="generalIcon" downIcon="generalIcon"/>
          <Button id="custom5BtnEndScreen" kClick="jsCall('customFunc5', mediaProxy.entry.id )" label="Custom 5" buttonType="onScreenButton" minWidth="60" labelPlacement="top" styleName="onScreenBtn" icon="generalIcon" upIcon="generalIcon" downIcon="generalIcon"/>
          <Button id="unmuteBtnEndScreen" kClick="sendNotification('changeVolume',1)" label="Unmute" buttonType="onScreenButton" minWidth="60" labelPlacement="top" styleName="onScreenBtn" upIcon="Button_upIcon_volumeBtn"/>
        </Tile>
        <Spacer height="100%"/>
      </VBox>
    </screen>
  </screens>
  <renderers>
    <renderer id="playlistItemRenderer">
      <HBox id="irCont" height="100%" width="100%" x="10" y="10" verticalAlign="top" paddingRight="20" styleName="Button_upSkin_default">
        <Image id="irImageIrScreen" height="48" width="72" url="{this.thumbnailUrl}"/>
        <VBox height="100%" width="100%" id="labelsHolder" verticalGap="0">
          <HBox id="nameAndDuration" width="100%" height="18">
            <Label id="irLinkIrScreen" height="18" width="100%" text="{this.name}" styleName="itemRendererLabel"/>
            <Label id="irDurationIrScreen" height="18" width="70" text="{formatDate(this.duration, 'NN:SS')}" styleName="itemRendererLabel"/>
          </HBox>
          <Label id="irDescriptionIrScreen" width="100%" height="18" text="{this.description}" styleName="itemRendererLabel"/>
          <Label id="irPlaysIrScreen" width="100%" height="18" text="Plays: {this.plays}" styleName="itemRendererLabel"/>
          <Label id="irRankIrScreen" width="100%" height="18" text="Rank: {this.rank}" styleName="itemRendererLabel"/>
          <Label id="irVotesIrScreen" width="100%" height="18" text="{this.votes} votes" styleName="itemRendererLabel"/>
          <Label id="irTagsIrScreen" width="100%" height="18" text="Tags: {this.tags}" styleName="itemRendererLabel"/>
          <Label id="irAdminTagsIrScreen" width="100%" height="18" text="Categories: {this.categories}" styleName="itemRendererLabel"/>
          <Label id="irCreatedAtIrScreen" width="100%" height="18" text="Created at: {formatDate(this.createdAt, 'dd/mm/yyy')}" styleName="itemRendererLabel"/>
          <Label id="irCreatedByIrScreen" width="100%" height="18" text="Created by: {this.userScreenName}" styleName="itemRendererLabel"/>
        </VBox>
      </HBox>
    </renderer>
  </renderers>
  <strings>
    <string key="ENTRY_CONVERTING" value="Entry is processing, please try again in a few minutes."/>
  </strings>
  <extraData/>
</layout>