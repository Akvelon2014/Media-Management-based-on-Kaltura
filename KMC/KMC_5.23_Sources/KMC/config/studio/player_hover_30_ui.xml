<layout id="full" name="player" skinPath="/content/uiconf/kaltura/kmc/appstudio/kdp3/falcon/skin/v3.6.5/KDP_Blue.swf">
  <HBox id="topLevel" width="100%" height="100%">
    <VBox id="player" width="100%" height="100%" styleName="black">
      <Plugin id="kalturaMix" width="0%" height="0%" includeInLayout="false" loadingPolicy="onDemand"/>
      <Plugin id="download" width="0%" height="0%" includeInLayout="false" loadingPolicy="noWait"/>
      <Plugin id="statistics" width="0%" height="0%" includeInLayout="false"/>

      <Plugin id="relatedEntries" width="0%" height="0%" sourceType="automatic" autoPlay="true" automaticPlaylistId="_KDP_RE_PL" entryId="{mediaProxy.entry.id}"/>
      <Plugin id="captureThumbnail" width="0%" height="0%" includeInLayout="false" loadingPolicy="noWait"/>
      <Plugin id="fader" width="0%" height="0%" includeInLayout="false" target="{controllersVbox}" hoverTarget="{PlayerHolder}" duration="0.5" autoHide="true"/>
      <!-- new vast plugin -->
      <Plugin id="vast" numPreroll="1" prerollInterval="2" prerollStartWith="3" prerollUrl="" overlayStartAt="" overlayInterval="" overlayUrl="" numPostroll="" postrollInterval="" postrollStartWith="" postrollUrl="" preSequence="1" postSequence="1" trackCuePoints="false"/>
      <Canvas id="PlayerHolder" height="100%" width="100%" styleName="black">
        <Video id="video" width="100%" height="100%"/>
        <!-- new notice message -->
        <Label id="noticeMessage" text="Video starts in {sequenceProxy.timeRemaining} seconds" height="30" width="100%" font="TimesNewRoman" styleName="movieName" dynamicColor="true" color1="0xFF0000" visible="{sequenceProxy.isAdLoaded}" includeInLayout="{sequenceProxy.isAdLoaded}"/>
        <VBox id="offlineMessageHolder" verticalAlign="middle" horizontalAlign="center" includeInLayout="false" width="100%" height="100%">
          <Spacer height="100%"/>
          <Spacer height="100%"/>
          <Label id="offlineMessage" styleName="offlineMessage" text="{mediaProxy.entry.offlineMessage}" visible="{mediaProxy.isOffline}" width="100%" height="30"/>
          <Spacer height="100%"/>
        </VBox>
        <VBox id="generalPluginContainer" width="100%" height="100%">
          <Spacer id="contentPusher" height="100%"/>
          <Plugin id="closedCaptionsOverPlayer" width="100%" height="15%" fontsize="15" bg="0x000000" fontFamily="Arial" fontColor="0xFFFFFF" opacity="0" path="closedCaptionsPlugin.swf"/>
          <Spacer id="contentPusherBottom" height="30"/>
        </VBox>
        <Screens id="screensLayer" width="100%" height="100%" mouseOverTarget="{PlayerHolder}" styleName="clickThrough" startScreenId="startScreen" startScreenOverId="startScreen" pauseScreenOverId="pauseScreen" pauseScreenId="pauseScreen" playScreenOverId="playScreen" endScreenId="endScreen" endScreenOverId="endScreen"/>
        <Watermark id="watermark" width="100%" height="100%" watermarkPath="http://www.kaltura.com/content/uiconf/kaltura/kmc/appstudio/kdp3/exampleWatermark.png" watermarkClickPath="http://www.kaltura.com/" watermarkPosition="topRight"/>
        <VBox id="relatedViewVBox" horizontalAlign="center" verticalAlign="middle" width="100%" height="100%" visible="{relatedView.visible}" includeInLayout="{relatedView.visible}" verticalGap="10" styleName="black">
          <VBox id="relatedVBox" width="100%" height="100%" maxWidth="800" maxHeight="600" paddingLeft="13" paddingRight="13" styleName="black" verticalAlign="bottom">
            <HBox id="upNextHbox" width="100%" height="35" paddingTop="10" horizontalGap="0" visible="{not(layoutProxy.isInFullScreen)}" includeInLayout="{not(layoutProxy.isInFullScreen)}">
              <Label id="upNextLabel" visible="{relatedEntries.autoPlay}" text="Up next in" width="63" height="20" styleName="Related_Text_UpNext"/>
              <Label id="upNextTime" visible="{relatedEntries.autoPlay}" text="{relatedEntries.timeRemaining}" width="25" height="20" styleName="Related_Text_Number"/>
              <Label id="upNextLabel2" visible="{relatedEntries.autoPlay}" text="secs" width="32" height="20" styleName="Related_Text_UpNext"/>
              <HBox id="pauseConteinueHBox" width="63" height="20" visible="{relatedEntries.autoPlay}" paddingLeft="0" verticalAlign="middle">
                <Button id="pauseRelatedBtn" width="30" height="20" label="Pause" visible="{relatedEntries.isTimerRunning}" includeInLayout="{relatedEntries.isTimerRunning}" kClick="sendNotification('pauseResumeRelatedTimer')" buttonType="labelButton" color1="0xCECECE" color2="0xFFFFFF"/>
                <Button id="playRelatedBtn" width="63" height="20" visible="{not(relatedEntries.isTimerRunning)}" includeInLayout="{not(relatedEntries.isTimerRunning)}" label="Continue" kClick="sendNotification('pauseResumeRelatedTimer')" buttonType="labelButton" color1="0xCECECE" color2="0xFFFFFF"/>
              </HBox>
              <HBox id="actionsHBox" width="100%" height="35" horizontalAlign="right" horizontalGap="5">
                <Button id="replayOnRelatedScreen" kClick="sendNotification('doSeek','0');sendNotification('doPlay')" label="Replay" labelPlacement="right" textPadding="5" minWidth="80" visible="{relatedView.showReplayBtn}" icon="replayIcon_Up" upIcon="replayIcon_Up" overIcon="replayIcon_Hover" downIcon="replayIcon_Down" disabeledIcon="replayIcon_Disabled"/>
               
                <Button id="kalturaShareBtnOnRelatedAuto" visible="{and(relatedEntries.autoPlay,relatedEntries.isTimerRunning)}" includeInLayout="{kalturaShareBtnOnRelatedAuto.visible}" kClick="sendNotification ('pauseResumeRelatedTimer');sendNotification('showAdvancedShare')" textPadding="5" label="Share" labelPlacement="right" minWidth="80" icon="shareIcon_Up" upIcon="shareIcon_Up" overIcon="shareIcon_Hover" downIcon="shareIcon_Down" disabeledIcon="shareIcon_Disabled"/>
                <Button id="kalturaShareBtnOnRelated" visible="{not(kalturaShareBtnOnRelatedAuto.visible)}" includeInLayout="{kalturaShareBtnOnRelated.visible}" kClick="sendNotification('showAdvancedShare')" label="Share" textPadding="5" labelPlacement="right" minWidth="80" icon="shareIcon_Up" upIcon="shareIcon_Up" overIcon="shareIcon_Hover" downIcon="shareIcon_Down" disabeledIcon="shareIcon_Disabled"/>
              </HBox>
            </HBox>
            <!-- different styles when in full screen -->
            <HBox id="upNextHboxFS" width="100%" height="50" paddingTop="10" horizontalGap="0" visible="{layoutProxy.isInFullScreen}" includeInLayout="{layoutProxy.isInFullScreen}">
              <Label id="upNextLabelFS" visible="{relatedEntries.autoPlay}" text="Up next in {relatedEntries.timeRemaining} secs" width="135" height="20" styleName="RelatedFS_Text_UpNext"/>
              <HBox width="85" height="20" visible="{relatedEntries.autoPlay}" paddingLeft="0" verticalAlign="middle">
                <Button id="pauseRelatedBtnFS" width="40" height="30" label="Pause" visible="{relatedEntries.isTimerRunning}" includeInLayout="{relatedEntries.isTimerRunning}" kClick="sendNotification('pauseResumeRelatedTimer')" buttonType="labelButton" color1="0xCECECE" color2="0xFFFFFF" styleName="FS"/>
                <Button id="playRelatedBtnFS" width="85" height="30" visible="{not(relatedEntries.isTimerRunning)}" includeInLayout="{not(relatedEntries.isTimerRunning)}" label="Continue" kClick="sendNotification('pauseResumeRelatedTimer')" buttonType="labelButton" color1="0xCECECE" color2="0xFFFFFF" styleName="FS"/>
              </HBox>
              <HBox width="100%" height="50" horizontalAlign="right" horizontalGap="5">
                <Button id="replayOnRelatedScreenFS" kClick="sendNotification('doSeek','0');sendNotification('doPlay')" label="Replay" labelPlacement="right" textPadding="5" minWidth="80" visible="{relatedView.showReplayBtn}" icon="replayIcon_Up" upIcon="replayIcon_Up" overIcon="replayIcon_Hover" downIcon="replayIcon_Down" disabeledIcon="replayIcon_Disabled"/>
               
                <Button id="kalturaShareBtnOnRelatedAutoFS" visible="{and(relatedEntries.autoPlay,relatedEntries.isTimerRunning)}" includeInLayout="{kalturaShareBtnOnRelatedAutoFS.visible}" kClick="sendNotification ('pauseResumeRelatedTimer');sendNotification('showAdvancedShare')" textPadding="5" label="Share" labelPlacement="right" minWidth="80" icon="shareIcon_Up" upIcon="shareIcon_Up" overIcon="shareIcon_Hover" downIcon="shareIcon_Down" disabeledIcon="shareIcon_Disabled"/>
                <Button id="kalturaShareBtnOnRelatedFS" visible="{not(kalturaShareBtnOnRelatedAutoFS.visible)}" includeInLayout="{kalturaShareBtnOnRelatedFS.visible}" kClick="sendNotification('showAdvancedShare')" label="Share" textPadding="5" labelPlacement="right" minWidth="80" icon="shareIcon_Up" upIcon="shareIcon_Up" overIcon="shareIcon_Hover" downIcon="shareIcon_Down" disabeledIcon="shareIcon_Disabled"/>
              </HBox>
            </HBox>
            <Spacer height="20"/>
            <Plugin id="relatedView" visible="false" width="100%" height="100%" dataProvider="{relatedEntries.dataProvider}" itemRenderer="relatedViewItemRenderer" viewType="tile"/>
          </VBox>
        </VBox>
        <VBox id="controllersVbox" width="100%" height="100%" verticalAlign="Bottom">
          <Spacer id="controllerPusher" width="100%" height="100%"/>
          <VBox id="ControllerScreenHolder" width="100%" height="80" verticalAlign="middle" styleName="Player_bg_hover">
            <!-- scrubber box. the scrubber and 2 timers -->
            <HBox id="scrubberBox" width="100%" height="28" verticalAlign="middle" paddingRight="10" paddingLeft="10">
              <HBox id="scrubberContainer" visible="{not(mediaProxy.isLive)}" width="100%" height="28" horizontalGap="0" paddingRight="9" verticalAlign="middle">
                <Timer id="timerControllerScreen1" width="45" height="25" styleName="timerProgressLeft" format="mm:ss" timerType="forwards"/>
                <Label id="timerControllerScreen1Label" text="/" width="10" height="25" color1="0xFFFFFF" dynamicColor="true" styleName="timerProgressRight"/>
                <Timer id="timerControllerScreen2" width="45" height="25" styleName="timerProgressRight" format="mm:ss" timerType="total"/>
                <VBox width="100%" height="28" paddingLeft="8" paddingRight="8" supportEnableGui="false">
                  <Scrubber id="scrubber" width="100%" height="100%"/>
                </VBox>
              </HBox>
              <Button id="kalturaLogo" minWidth="50" kClick="navigate('http://www.kaltura.com')" styleName="controllerScreen" icon="kalturaLogo"/>
            </HBox>
            <HBox id="ControllerScreen" width="100%" height="100%" horizontalGap="15" paddingLeft="25" paddingBottom="8" paddingRight="25" verticalAlign="middle">
              <Button id="playBtnControllerScreen" width="20" height="30" command="play" icon="playIcon_up" overIcon="playIcon_Hover" downIcon="playIcon_Down" disabeledIcon="playIcon_Disabled" selectedUpIcon="pauseIcon_Up" selectedOverIcon="pauseIcon_Hover" selectedDownIcon="pauseIcon_Down" selectedDisabledIcon="pauseIcon_Disabled"/>
              <Button id="liveToggleStatus" toggle="true" color1="0xFF0000" color2="0xFF0000" upIcon="onAirIcon_Up" overIcon="onAirIcon_Hover" downIcon="onAirIcon_Down" disabeledIcon="onAirIcon_Disabled" selectedUpIcon="offlineIcon_up" selectedOverIcon="offlineIcon_Hover" selectedDownIcon="offlineIcon_Down" selectedDisabledIcon="offlineIcon_Disabled" isSelected="{mediaProxy.isOffline}" visible="{mediaProxy.isLive}" includeInLayout="{mediaProxy.isLive}" mouseEnable="false" useHandCursor=""/>
              <Spacer id="buttonsPusher" width="100%"/>
              <!-- Combo box for captions control -->
              <ComboBox id="ccUnderComboBox" width="90" height="28" styleName="_kdp" selectedIndex="{closedCaptionsUnderPlayer.currentCCFileIndex}" kevent_change="sendNotification( 'closedCaptionsSelected' , ccUnderComboBox.selectedItem)" dataProvider="{closedCaptionsUnderPlayer.availableCCFilesLabels}" prompt="Captions" visible="{closedCaptionsUnderPlayer.hasCaptions}" includeInLayout="{closedCaptionsUnderPlayer.hasCaptions}"/>
              <Button id="kalturaShareBtnControllerScreen" kClick="sendNotification('showAdvancedShare')" height="30" styleName="controllerScreen" icon="shareIcon_Up" overIcon="shareIcon_Hover" downIcon="shareIcon_Down" disabeledIcon="shareIcon_Disabled" k_buttonType="falconButtonIconControllerArea" uiconfId="" buttonType="normal" font="Arial"/>
              <Button id="flagBtnControllerScreen" kClick="sendNotification('flagForReview')" height="30" styleName="controllerScreen" icon="flagIcon_Up" overIcon="flagIcon_Hover" downIcon="flagIcon_Down" disabeledIcon="flagIcon_Disabled"/>
              <!--Button id="editBtnControllerScreen" label="" buttonType="iconButton" focusRectPadding="0"
						kClick="jsCall('gotoEditorWindow', mediaProxy.entry.id )" height="22"
						styleName="controllerScreen" icon="remixIcon" tooltip="Open editor"/-->
              <Button id="uploadBtnControllerScreen" kClick="jsCall('gotoContributorWindow', mediaProxy.entry.id )" height="30" styleName="controllerScreen" icon="uploadIcon_up" overIcon="uploadIcon_Hover" downIcon="uploadIcon_Down" disabeledIcon="uploadIcon_Disabled"/>
              <Button id="downloadBtnControllerScreen" kClick="sendNotification('doDownload')" height="30" styleName="controllerScreen" icon="downloadIcon_up" overIcon="downloadIcon_Hover" downIcon="downloadIcon_Down" disabeledIcon="downloadIcon_Disabled"/>
              <Button id="captureThumbBtnControllerScreen" kClick="sendNotification('captureThumbnail')" height="30" styleName="controllerScreen" icon="thumbIcon_up" overIcon="thumbIcon_Hover" downIcon="thumbIcon_Down" disabeledIcon="thumbIcon_Disabled"/>
              <FlavorCombo id="flavorComboControllerScreen" width="80" height="28" streamerType="{configProxy.flashvars.streamerType}" flavorDataProvider="{mediaProxy.kalturaMediaFlavorArray}" styleName="_kdp" hdOn="HD On" hdOff="HD Off" selectedMessage="" autoMessage="Automatically switches between bitrates" preferedFlavorBR="{mediaProxy.preferedFlavorBR}" tooltip="{flavorComboControllerScreen.selectedMessage}" usePixels="{mediaProxy.displayFlavorPixels}" isAutoSwitch="{mediaProxy.isFlavorSwitching}"/>
              <VolumeBar id="volumeBar" width="30" height="30" icon="VolumeIcon_Up" buttonUpSkin="VolumeIcon_Up" styleName="controllerScreen" buttonOverSkin="VolumeIcon_Hover" buttonDownSkin="VolumeIcon_Down" buttonDisabledSkin="VolumeIcon_Disabled" buttonSelectedUpSkin="MuteIcon_up" buttonSelectedOverSkin="MuteIcon_Hover" buttonSelectedDownSkin="MuteIcon_Down" buttonSelectedDisabledSkin="MuteIcon_Disabled"/>
              <Button id="fullScreenBtnControllerScreen" command="fullScreen" height="30" styleName="controllerScreen" allowDisable="false" icon="closeFullScreenIcon_Up" overIcon="closeFullScreenIcon_Hover" downIcon="closeFullScreenIcon_Down" disabeledIcon="closeFullScreenIcon_Disabled" selectedUpIcon="openFullScreenIcon_Up" selectedOverIcon="openFullScreenIcon_Hover" selectedDownIcon="openFullScreenIcon_Down" selectedDisabledIcon="openFullScreenIcon_Disabled"/>
              <Button id="custom1BtnControllerScreen" height="30" kClick="jsCall('customFunc1', mediaProxy.entry.id )" label="Custom 1" styleName="controllerScreen" icon="CustomIcon_Up" overIcon="CustomIcon_Hover" downIcon="CustomIcon_Down" disabeledIcon="CustomIcon_Disabled"/>
              <Button id="custom2BtnControllerScreen" height="30" kClick="jsCall('customFunc2', mediaProxy.entry.id )" label="Custom 2" styleName="controllerScreen" icon="CustomIcon_Up" overIcon="CustomIcon_Hover" downIcon="CustomIcon_Down" disabeledIcon="CustomIcon_Disabled"/>
              <Button id="custom3BtnControllerScreen" height="30" kClick="jsCall('customFunc3', mediaProxy.entry.id )" label="Custom 3" styleName="controllerScreen" icon="CustomIcon_Up" overIcon="CustomIcon_Hover" downIcon="CustomIcon_Down" disabeledIcon="CustomIcon_Disabled"/>
              <Button id="custom4BtnControllerScreen" height="30" kClick="jsCall('customFunc4', mediaProxy.entry.id )" label="Custom 4" styleName="controllerScreen" icon="CustomIcon_Up" overIcon="CustomIcon_Hover" downIcon="CustomIcon_Down" disabeledIcon="CustomIcon_Disabled"/>
              <Button id="custom5BtnControllerScreen" height="30" kClick="jsCall('customFunc5', mediaProxy.entry.id )" label="Custom 5" styleName="controllerScreen" icon="CustomIcon_Up" overIcon="CustomIcon_Hover" downIcon="CustomIcon_Down" disabeledIcon="CustomIcon_Disabled"/>
            </HBox>
          </VBox>
        </VBox>
        <!-- new overlay plugin -->
        <Plugin id="overlay" swfUrls="{vast.overlays}" overlayStartAt="{vast.overlayStartAt}" overlayInterval="{vast.overlayInterval}" displayDuration="" width="100%" height="100%"/>
        <!-- new bumper plugin -->
        <Plugin id="bumper" bumperEntryID="0_c2gx2xjm" clickurl="http://www.kaltura.com" lockUI="true" playOnce="false" preSequence="1" postSequence="1" width="100%" height="100%"/>
        <!-- new skip button -->
        <Button id="skipBtn" visible="{and(sequenceProxy.isAdSkip, sequenceProxy.isAdLoaded}" includeInLayout="{and(sequenceProxy.isAdSkip,sequenceProxy.isAdLoaded)}" type="labelButton" color1="0xFFFF00" color2="0xFF00FF" styleName="controllerScreen" height="30" width="20" allowDisable="false" label="Skip Ad >" kClick="sendNotification('sequenceSkipNext')"/>
        <Plugin id="plymedia" width="100%" height="100%" useHost="false" loadingPolicy="noWait"/>
        <HBox id="ccOverComboBoxWrapper" horizontalAlign="left" width="100%" height="100%" paddingLeft="5" paddingTop="5">
          <Plugin id="captionsOverFader" width="0%" height="0%" includeInLayout="false" target="{ccOverComboBoxWrapper}" hoverTarget="{PlayerHolder}" duration="0.5" autoHide="true" path="faderPlugin.swf"/>
          <ComboBox id="ccOverComboBox" width="90" styleName="_kdp" selectedIndex="{closedCaptionsOverPlayer.currentCCFileIndex}" kevent_change="sendNotification( 'closedCaptionsSelected' , ccOverComboBox.selectedItem)" dataProvider="{closedCaptionsOverPlayer.availableCCFilesLabels}" prompt="Captions" visible="{closedCaptionsOverPlayer.hasCaptions}"/>
        </HBox>
        <Plugin id="kalturaShare" uiconfId="8700151" width="100%" height="100%"/>
        <!-- customAd plugin -->
        <Plugin id="customAd" path="" preSequence="1" postSequence="1" width="100%" height="100%"/>
        <!-- moderation plugin -->
        <Plugin id="moderation" width="100%" height="100%" header="Report this content as Inapproriate" text="Please describe your concern about the video, so that we can review it and determine whether it isn't appropriate for all viewers."/>
      </Canvas>
      <!-- closed captions plugin under the player -->
      <Plugin id="closedCaptionsUnderPlayer" width="100%" height="15%" fontsize="15" bg="0x000000" fontFamily="Arial" fontColor="0xFFFFFF" opacity="0" path="closedCaptionsPlugin.swf"/>
    </VBox>
  </HBox>
  <screens>
    <screen id="startScreen">
      <VBox id="startContainer" width="100%" height="100%" verticalAlign="middle" horizontalAlign="center">
        <Tile id="startTile" width="100%" verticalGap="10" verticalAlign="middle" horizontalAlign="center">
          <Button id="onVideoPlayBtnStartScreen" command="play" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" label="Play" styleName="onScreenBtn" upIcon="OnVideo_playIcon_Up" overIcon="OnVideo_playIcon_Hover" downIcon="OnVideo_playIcon_Down" disabeledIcon="OnVideo_playIcon_Disabled"/>
          <Button id="kalturaShareBtnStartScreen" kClick="sendNotification('showAdvancedShare')" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" label="Share" styleName="onScreenBtn" icon="OnVideo_shareIcon_Up" upIcon="OnVideo_shareIcon_Up" overIcon="OnVideo_shareIcon_Hover" downIcon="OnVideo_shareIcon_Down" disabeledIcon="OnVideo_shareIcon_Disabled" k_buttonType="falconButtonIconControllerArea" uiconfId="" buttonType="normal" font="Arial"/>
          <Button id="flagBtnStartScreen" kClick="sendNotification('flagForReview')" label="Flag" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_flagIcon_Up" upIcon="OnVideo_flagIcon_Up" overIcon="OnVideo_flagIcon_Hover" downIcon="OnVideo_flagIcon_Down" disabeledIcon="OnVideo_flagIcon_Disabled"/>
          <Button id="uploadBtnStartScreen" kClick="jsCall('gotoContributorWindow', mediaProxy.entry.id )" label="Upload" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_uploadIcon_up" upIcon="OnVideo_uploadIcon_up" overIcon="OnVideo_uploadIcon_Hover" downIcon="OnVideo_uploadIcon_Down" disabeledIcon="OnVideo_uploadIcon_Disabled"/>
          <Button id="downloadBtnStartScreen" kClick="sendNotification('doDownload')" label="Download" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_downloadIcon_Up" upIcon="OnVideo_downloadIcon_Up" overIcon="OnVideo_downloadIcon_Hover" downIcon="OnVideo_downloadIcon_Down" disabeledIcon="OnVideo_downloadIcon_Disabled"/>
          <Button id="captureThumbBtnStartScreen" kClick="sendNotification('captureThumbnail')" label="Thumb" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_thumbIcon_Up" upIcon="OnVideo_thumbIcon_Up" overIcon="OnVideo_thumbIcon_Hover" downIcon="OnVideo_thumbIcon_Down" disabeledIcon="OnVideo_thumbIcon_Disabled"/>
          <Button id="fullScreenBtnStartScreen" command="fullScreen" label="Fullscreen" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_closeFullScreenIcon_Up" upIcon="OnVideo_closeFullScreenIcon_Up" overIcon="OnVideo_closeFullScreenIcon_Hover" downIcon="OnVideo_closeFullScreenIcon_Down" disabeledIcon="OnVideo_closeFullScreenIcon_Disabled" selectedUpIcon="OnVideo_openFullScreenIcon_Up" selectedOverIcon="OnVideo_openFullScreenIcon_Hover" selectedDownIcon="OnVideo_openFullScreenIcon_Down" selectedDisabledIcon="OnVideo_openFullScreenIcon_Disabled"/>
          <Button id="custom1BtnStartScreen" label="Custom 1" kClick="jsCall('customFunc1', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom2BtnStartScreen" label="Custom 2" kClick="jsCall('customFunc2', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom3BtnStartScreen" label="Custom 3" kClick="jsCall('customFunc3', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom4BtnStartScreen" label="Custom 4" kClick="jsCall('customFunc4', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom5BtnStartScreen" label="Custom 5" kClick="jsCall('customFunc5', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="unmuteBtnStartScreen" kClick="sendNotification('changeVolume',1);sendNotification('volumeChangedEnd',1)" label="Unmute" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_VolumeIcon_up" upIcon="OnVideo_VolumeIcon_up" overIcon="OnVideo_VolumeIcon_Hover" downIcon="OnVideo_VolumeIcon_Down" disabeledIcon="OnVideo_VolumeIcon_Disabled"/>
        </Tile>
      </VBox>
    </screen>
    <screen id="pauseScreen">
      <VBox id="pauseContainer" width="100%" height="100%" verticalAlign="middle" horizontalAlign="center">
        <Tile id="pauseTile" width="100%" verticalGap="10" verticalAlign="middle" horizontalAlign="center">
          <Button id="onVideoPlayBtnPauseScreen" command="play" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" label="Play" styleName="onScreenBtn" upIcon="OnVideo_playIcon_Up" overIcon="OnVideo_playIcon_Hover" downIcon="OnVideo_playIcon_Down" disabeledIcon="OnVideo_playIcon_Disabled"/>
          <Button id="kalturaShareBtnPauseScreen" kClick="sendNotification('showAdvancedShare')" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" label="Share" styleName="onScreenBtn" icon="OnVideo_shareIcon_Up" upIcon="OnVideo_shareIcon_Up" overIcon="OnVideo_shareIcon_Hover" downIcon="OnVideo_shareIcon_Down" disabeledIcon="OnVideo_shareIcon_Disabled" k_buttonType="falconButtonIconControllerArea" uiconfId="" buttonType="normal" font="Arial"/>
          <Button id="flagBtnPauseScreen" kClick="sendNotification('flagForReview')" label="Flag" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_flagIcon_Up" upIcon="OnVideo_flagIcon_Up" overIcon="OnVideo_flagIcon_Hover" downIcon="OnVideo_flagIcon_Down" disabeledIcon="OnVideo_flagIcon_Disabled"/>
          <Button id="uploadBtnPauseScreen" kClick="jsCall('gotoContributorWindow', mediaProxy.entry.id )" label="Upload" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_uploadIcon_up" upIcon="OnVideo_uploadIcon_up" overIcon="OnVideo_uploadIcon_Hover" downIcon="OnVideo_uploadIcon_Down" disabeledIcon="OnVideo_uploadIcon_Disabled"/>
