<root>
	<locale>
		<path>locale/{locale}/studio_{locale}_ResourceModule.swf</path>
		<language>en_US</language>
	</locale>
	
	<!-- current templates file -->
	<uiconf>@@StudioTemplatesUIConfID@@</uiconf>
	<content>
		<!-- entry to show in single player templates -->
		<entryId>_KMCLOGO1</entryId>
		<!-- playlists to show in playlist/multiplaylist player templates -->
		<playlists>
			<playlist>
				<url>/index.php/partnerservices2/executeplaylist?uid=&partner_id={pid}&subp_id={spid}&format=8&ks={ks}&playlist_id=_KMCSPL1</url>
				<name>playlist1</name>
			</playlist>
			<playlist>
				<url>/index.php/partnerservices2/executeplaylist?uid=&partner_id={pid}&subp_id={spid}&format=8&ks={ks}&playlist_id=_KMCSPL2</url>
				<name>playlist2</name>
			</playlist>
		</playlists>
	</content>
