    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here
var insertSql;
var updateSql;
insertSql = &quot;INSERT INTO kalturadw.dwh_dim_&quot;+
			table_name+&quot; (&quot;+table_name+&quot;_id,&quot;+table_name+&quot;_name, ri_ind) 	SELECT &quot;+
			table_name+&quot;_id,&quot;+table_name+&quot;_name, 0 ri_ind 	FROM kalturadw_bisources.bisources_&quot;+
			table_name+&quot; WHERE &quot;+table_name+&quot;_id NOT IN (SELECT &quot;+
			table_name+&quot;_id FROM kalturadw.dwh_dim_&quot;+table_name+&quot;)&quot;;
updateSql = &quot;UPDATE  kalturadw.dwh_dim_&quot;+
			table_name+&quot; dw,kalturadw_bisources.bisources_&quot;+
			table_name+&quot; bi SET dw.&quot;+table_name+&quot;_name = bi.&quot;+
			table_name+&quot;_name, dw.ri_ind=0 WHERE dw.&quot;+table_name+&quot;_id = bi.&quot;+
			table_name+&quot;_id	AND dw.&quot;+table_name+&quot;_name &lt;&gt; bi.&quot;+table_name+&quot;_name&quot;;
</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>insertSql</name>
