,UPPER(state) state
,UPPER(city) city
, &apos;country&apos; country_name
, &apos;state&apos; state_name
, &apos;city&apos; city_name
FROM kuser
WHERE NULLIF(country,&apos;&apos;) IS NOT NULL
AND updated_at between STR_TO_DATE(&apos;${LastUpdatedAt}&apos;, &apos;%Y&#47;%m&#47;%d %H:%i:%S.%f&apos;)
and STR_TO_DATE(&apos;${OperationalReplicationSyncedAt}&apos;, &apos;%Y&#47;%m&#47;%d %H:%i:%S.%f&apos;)
