        <jsScript_script>var iplong = ip2number(user_ip);

function ip2number (ip_address) 
{
	numeric_ip = 0;
	if (ip_address) 
	{
		ip_array = ip_address.split(&apos;.&apos;);
		if (ip_array.length == 4) 
		{
			with (Math) 
			{
   		    	numeric_ip = ip_array[0]*pow(256,3)+ip_array[1]*pow(256,2)+ip_array[2]*256+ip_array[3]*1;
			}
		}
	}
	return numeric_ip;
}

var ipLookupSQL = &quot;SELECT country_id, location_id FROM kalturadw.dwh_dim_ip_ranges WHERE ip_from = (SELECT MAX(ip_from) FROM kalturadw.dwh_dim_ip_ranges WHERE &quot; + iplong + &quot;&gt;= ip_from)&quot;;</jsScript_script>
