  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>KalturaDW</connection>
    <sql>SELECT day_id, 0 hour_id
FROM kalturadw.dwh_dim_time
WHERE NOT EXISTS (
	SELECT DISTINCT(date_id) 
	FROM kalturadw.aggr_managment 
	WHERE aggr_name = &apos;storage_usage&apos;
	AND date_id = day_id
)
AND day_id BETWEEN (SELECT MAX(date_id) date_id FROM kalturadw.aggr_managment
	 WHERE aggr_name = &apos;storage_usage&apos;
	 AND start_time IS NOT NULL 
	 AND end_time IS NOT NULL 
	 AND end_time &gt;= start_time
	 AND start_time &gt;= data_insert_time
	 AND date_id &gt; (DATE(NOW()) - INTERVAL ${ResetStorageMaxDaysBack} DAY)*1) AND 
	 (SELECT MIN(date_id) date_id FROM kalturadw.aggr_managment
	 WHERE aggr_name = &apos;storage_usage&apos;
	 AND (data_insert_time &gt; IFNULL(start_time, DATE(19700101)) OR IFNULL(end_time,DATE(19700101)) &lt; start_time)
	 AND date_id &gt; (DATE(NOW()) - INTERVAL ${ResetStorageMaxDaysBack} DAY)*1)
</sql>
