  <step>
    <name>Read days to calc</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>KalturaDW</connection>
    <sql>SELECT distinct date_id,aggr_name	 
FROM kalturadw.aggr_managment, kalturadw_ds.parameters p
WHERE 	date_id + INTERVAL hour_id HOUR &lt; p.date_value
	AND (ifnull(start_time,date(19700101)) &lt; data_insert_time 
			OR
			start_time &gt; ifnull(end_time,date(19700101)) &#47;* Handle Failed aggregations*&#47;)
		AND p.id = 2 &#47;*Update dimensions parameter*&#47;
		and aggr_name = ?
ORDER BY date_id</sql>
    <limit>0</limit>
    <lookup>Select aggr_name</lookup>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
