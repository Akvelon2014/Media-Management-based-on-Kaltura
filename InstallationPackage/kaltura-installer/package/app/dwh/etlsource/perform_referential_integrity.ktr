    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here
var insertSql;

lastUpdatedAt = &apos;STR_TO_DATE(\&apos;&apos;+last_updated_at+&apos;\&apos;, \&apos;%Y&#47;%m&#47;%d %H:%i:%S.%f\&apos;)&apos;;

var dateFilter = date_column_name == &apos;&apos; || date_column_name == null ? 
					&apos;a.&apos;+date_id_column_name+&apos;&gt;=DATE(&apos;+lastUpdatedAt+&apos;)*1&apos; :
					&apos;a.&apos;+date_column_name+&apos;&gt;=&apos;+lastUpdatedAt;
	
insertSql = &quot;INSERT INTO &quot; + reference_table + &quot; (&quot; + reference_column + &quot;,&quot; + default_fields + &quot;,ri_ind) &quot; +
			&quot;SELECT DISTINCT a.&quot; + column_name + &quot;,&quot; + default_values + &quot;,1 &quot; + 
			&quot;FROM kalturadw.&quot; + table_name + &quot; a LEFT OUTER JOIN kalturadw.&quot; + reference_table + &quot; b &quot; + 
			&quot;ON a.&quot;	+ column_name+&quot; = b.&quot; + reference_column +
			&quot; WHERE &quot; + dateFilter + &quot; AND b.&quot; + reference_column + &quot; IS NULL &quot; + 
			&quot;AND a.&quot; + column_name + &quot; IS NOT NULL;&quot;;</jsScript_script>
      </jsScript>      <jsScript>        <jsScript_type>-1</jsScript_type>
        <jsScript_name>indexOf_Sample</jsScript_name>
        <jsScript_script>&#47;&#47; Perform the indexOf function
&#47;&#47; 
&#47;&#47; Usage:
&#47;&#47; indexOf(string, substring);
&#47;&#47; indexOf(string, substring, fromIndex);
&#47;&#47;
&#47;&#47; 2007-09-25
&#47;&#47;
var str1= &quot;Hello Pentaho!&quot;;
var str2= indexOf(str1, &quot;Pentaho&quot;);
var str3= indexOf(str1, &quot;o&quot;, 7);
Alert(&quot;Input : &quot; + str1);
Alert(&quot;Index of &apos;Pentaho&apos; : &quot; + str2);
Alert(&quot;index of &apos;o&apos;, search from position 7 : &quot; + str3);
</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>insertSql</name>
