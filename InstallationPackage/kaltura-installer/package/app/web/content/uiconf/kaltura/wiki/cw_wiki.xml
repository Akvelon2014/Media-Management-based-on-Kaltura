	<ImportTypesConfig>
		<searchConfig>
		</searchConfig>
		<taggingConfig>
			<minTitleLen>1<minTitleLen/>
			<maxTitleLen>60<maxTitleLen/>
		</taggingConfig>

	</ImportTypesConfig>
-->
	<UIConfigList>
		<UIConfig>
			<target>
				ContributionWizard.swf
			</target>
			<cssUrl>/content/uiconf/kaltura/styles_ContributionWizard_default.swf</cssUrl>
			<localeUrl>/content/uiconf/kaltura/en_US_ContributionWizard.swf</localeUrl>
		</UIConfig>
	</UIConfigList>

	<StartupDefaults>
		<NavigationProperties>
			<enableIntroScreen>true</enableIntroScreen>
			<enableTagging>true</enableTagging>
			<showCloseButton>true</showCloseButton>
		</NavigationProperties>


		<gotoScreen>
			<mediaType/>
			<mediaProviderId>-1</mediaProviderId>
		</gotoScreen>

		<SingleContribution>false</SingleContribution>

		<permissions>
			<alwaysShowPermission>false</alwaysShowPermission>
			<defaultPermissionLevel>1</defaultPermissionLevel>
		</permissions>

		<autoTOUConfirmation>false</autoTOUConfirmation>
	</StartupDefaults>

</ContributionWizard>
