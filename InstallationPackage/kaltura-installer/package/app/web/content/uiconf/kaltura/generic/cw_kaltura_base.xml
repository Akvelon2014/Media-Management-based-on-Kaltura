<?xml version="1.0"?>
<xml>
<provider id="upload" code="1">
	<authMethodList>
	  <authMethod id="1">1</authMethod>
	  <authMethod id="2">1</authMethod>
	</authMethodList>
	<moduleUrl>com/kaltura/contributionWizard/view/importViews/upload/UploadView.swf</moduleUrl>
	
	<fileFilters>
		<filter id="video">
			<allowedTypes>flv,asf,qt,mov,mpg,avi,wmv</allowedTypes>	
		</filter>
		<filter id="audio">
		  <allowedTypes>flv,asf,wmv,qt,mov,mpg,avi,mp3,wav</allowedTypes>
		</filter>
		<filter id="image">
		  <allowedTypes>jpg,bmp,png,gif,tiff</allowedTypes>
		</filter>
	</fileFilters>
</provider>


<provider id="webcam" code="2">
	<authMethodList>
		<authMethod>1</authMethod>
	</authMethodList>
	<moduleUrl>com/kaltura/contributionWizard/view/importViews/webcam/WebcamView.swf</moduleUrl>
	<logo/>
	<customData>
		<serverUrl>rtmp://{HOST_NAME}/oflaDemo</serverUrl>
	</customData>
</provider>

