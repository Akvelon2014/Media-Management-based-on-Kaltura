						<Button id="shareBtnControllerScreen" k_buttonType="buttonIconControllerArea" command="gigyaButton" label="Share" styleName="shareBtn" tooltip="share with friends"/>
						<Spacer width="3"/>
						<Button id="uploadBtnControllerScreen" width="autoSize" height="autoSize" k_buttonType="buttonControllerArea" k_click="jsCall('handleGotoContribWizard', widgetId, entryId); pauseMedia()" label="Upload" styleName="uploadBtn" tooltip="Add additional videos, images or audio files to this video"/>
