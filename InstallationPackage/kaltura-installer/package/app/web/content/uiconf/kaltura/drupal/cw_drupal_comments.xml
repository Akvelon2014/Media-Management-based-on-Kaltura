<ContributionWizard>
<MediaProviders>
  <service>
    <id>2</id>
    <type>webcam</type>
    <authMethodList>
      <authMethod>1</authMethod>
    </authMethodList>
    <moduleUrl>WebcamView.swf</moduleUrl>
    <name>Webcam</name>
    <logo/>
    <customData>
      <serverUrl>rtmp://{HOST_NAME}/oflaDemo</serverUrl>
    </customData>
    <media>
      <type>video</type>
    </media>
  </service>
</MediaProviders>

<UIConfigList>

  <UIConfig>
    <target>
		ContributionWizard.swf
    </target>
    <cssUrl>/content/uiconf/kaltura/drupal/styles_ContributionWizard.swf</cssUrl>
  </UIConfig>

</UIConfigList>

<StartupDefaults>
	<NavigationProperties>
		<enableIntroScreen>false</enableIntroScreen>
		<enableTagging>true</enableTagging>
	</NavigationProperties>
	<gotoScreen>
		<mediaType>video</mediaType>
		<mediaProviderId>2</mediaProviderId>
	</gotoScreen>
	<SingleContribution>true</SingleContribution>
</StartupDefaults>

</ContributionWizard>
