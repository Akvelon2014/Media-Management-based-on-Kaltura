			<provider id="upload" name="upload" code="1">
				<authMethodList>
				  <authMethod type="1" />
				</authMethodList>
				<moduleUrl>UploadView.swf</moduleUrl>
				
				<fileFilters>
					<filter type="swf">
						<allowedTypes>swf</allowedTypes>	
					</filter>
				</fileFilters>
			</provider>
		</media>
		
	</mediaTypes>
	

	<UIConfigList>
		<UIConfig>
		    <target>
				ContributionWizard.swf
		    </target>
		    <cssUrl>/content/uiconf/kcweditor/styles/light/default.swf</cssUrl>
		</UIConfig>
	</UIConfigList>

	<ImportTypesConfig>
		<taggingConfig>
			<minTitleLen>1</minTitleLen>
			<maxTitleLen>2000</maxTitleLen>
			<minTagsLen>0</minTagsLen>
			<maxTagsLen>2000</maxTagsLen>
		</taggingConfig>
	</ImportTypesConfig>
	
	<StartupDefaults>
			<SingleContribution>false</SingleContribution>
			<autoTOUConfirmation>true</autoTOUConfirmation>
			<showLogoImage>false</showLogoImage>
			<gotoScreen>
				<mediaType>video</mediaType>
				<mediaProviderName>webcam</mediaProviderName>
			</gotoScreen>
			
			<NavigationProperties>
				<showCloseButton>true</showCloseButton>
				<enableIntroScreen>true</enableIntroScreen>
				<enableTagging>true</enableTagging>
			</NavigationProperties>
	</StartupDefaults>
	
	<webcamParams>
		<keyFrameInterval></keyFrameInterval>
		<width></width>
		<height></height>
		<framerate></framerate>
		<favorArea></favorArea>
		<bandwidth></bandwidth>
		<quality></quality>
	</webcamParams>
	
	<limitations>
		<upload>
			<singleFileSize min="-1" max="-1" />
			<numFiles min="-1" max="-1" />
			<totalFileSize min="-1" max="-1" />
		</upload>
		<search>
			<numFiles min="-1" max="-1" />
		</search>
	</limitations>
	
	<!-- Extra XML for KCWEditor only -->
	<KCWEditor>
