			<localeUrl id="es_ES" name="Spanish ES">/content/uiconf/kcweditor/locales/es_ES/es_ES_locales.swf</localeUrl>
			<localeUrl id="de_DE" name="Deutsch DE">/content/uiconf/kcweditor/locales/de_DE/de_DE_locales.swf</localeUrl>
