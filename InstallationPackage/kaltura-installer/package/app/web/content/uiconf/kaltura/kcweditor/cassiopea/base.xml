			<cssUrl id="light_new" name="New Light (for version 2.1.4 and up)" tooltip="New Light (for version 2.1.4 and up)">/content/uiconf/kaltura/kmc/kcw/v2.1.4/styles.swf</cssUrl>
			<cssUrl id="light" name="Default Light (for earlier versions than 2.1.4)" tooltip="Default Light (for earlier versions than 2.1.4)">/content/uiconf/kcweditor/styles/light/default.swf</cssUrl>
			<cssUrl id="dark" name="Default Dark">/content/uiconf/kcweditor/styles/dark/default.swf</cssUrl>
			<cssUrl id="blue" name="Default Light Blue">/content/uiconf/kaltura/wordpress/styles_ContributionWizard.swf</cssUrl>
			<cssUrl id="lightNoTabs" name="NoTabs Light">/content/uiconf/kcweditor/styles/light/default_notabs.swf</cssUrl>
			<cssUrl id="darkNoTabs" name="x NoTabs Dark">/content/uiconf/kcweditor/styles/dark/default_notabs.swf</cssUrl>
			<cssUrl id="lightSpanish" name="Spanish Light">/content/uiconf/kcweditor/styles/light/spanish.swf</cssUrl>
			<cssUrl id="lightSpanishNotabs" name="Spanish Light no tabs">/content/uiconf/kcweditor/styles/light/spanish_notabs.swf</cssUrl>
