				</authMethodList>
			</provider>
			
			<provider id="webcam" name="webcam" code="2">
				<authMethodList>
				  <authMethod type="1" />
				</authMethodList>
				<moduleUrl>WebcamView.swf</moduleUrl>
				<customData>
				  <serverUrl>rtmp://{HOST_NAME}/oflaDemo</serverUrl>
