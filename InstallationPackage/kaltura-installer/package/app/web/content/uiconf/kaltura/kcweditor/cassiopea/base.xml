<?xml version="1.0"?>
<kcw>
	<mediaTypes>
		<media type="video">
			<provider id="upload" name="upload" code="1">
				<authMethodList>
				  <authMethod type="1" />
				</authMethodList>
				<moduleUrl>UploadView.swf</moduleUrl>
				<fileFilters>
					<filter type="video">
						<allowedTypes>flv,asf,qt,mov,mpg,avi,wmv,mp4,3gp</allowedTypes>	
					</filter>
				</fileFilters>
			</provider>
			
			<provider id="thissite" name="thissite" code="23">
				<moduleUrl>SearchView.swf</moduleUrl>
				<authMethodList>
					<authMethod type="1" />
				</authMethodList>
				<tokens>
			     <token>
			      <name>extra_data</name>
			      <value>$partner_id</value>
			     </token>
			    </tokens>
			</provider>
			
			<provider id="youtube" name="youtube" code="4" addsearch="false">
				<moduleUrl>SearchView.swf</moduleUrl>
				<authMethodList>
					<authMethod type="1" />
					<authMethod type="2" searchable="false" />
				</authMethodList>
			</provider>			
			
			<provider id="mySpace" name="mySpace" code="7">
				<moduleUrl>SearchView.swf</moduleUrl>
				<authMethodList>
					<authMethod type="1" />
