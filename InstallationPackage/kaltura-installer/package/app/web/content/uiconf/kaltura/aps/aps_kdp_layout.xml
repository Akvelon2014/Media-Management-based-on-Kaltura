				<primaryColor>0xFF6600</primaryColor>
				<secondaryColor>0x00FF99</secondaryColor>
				<cssSelector componentId="TopTitle" newClass="a" />
				<cssSelector componentId="controllerBox" newClass="a" />
			</theme>
			<theme id="theme2">
