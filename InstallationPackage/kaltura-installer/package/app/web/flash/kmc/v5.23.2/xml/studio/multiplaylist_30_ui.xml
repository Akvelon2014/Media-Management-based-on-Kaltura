          <Button id="custom2BtnPauseScreen" label="Custom 2" kClick="jsCall('customFunc2', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom3BtnPauseScreen" label="Custom 3" kClick="jsCall('customFunc3', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom4BtnPauseScreen" label="Custom 4" kClick="jsCall('customFunc4', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
          <Button id="custom5BtnPauseScreen" label="Custom 5" kClick="jsCall('customFunc5', mediaProxy.entry.id )" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_CustomIcon_Up" upIcon="OnVideo_CustomIcon_Up" overIcon="OnVideo_CustomIcon_Hover" downIcon="OnVideo_CustomIcon_Down" disabeledIcon="OnVideo_CustomIcon_Disabled"/>
