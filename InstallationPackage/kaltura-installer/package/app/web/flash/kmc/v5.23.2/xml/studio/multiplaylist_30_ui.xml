  <renderers>
    <renderer id="playlistItemRenderer">
      <HBox id="irCont" width="100%" height="100%" styleName="List_background_default" horizontalGap="5" verticalAlign="middle" paddingLeft="8" paddingRight="8">
        <Canvas id="irCanvasImageIrScreen" width="88" height="66" styleName="List_background_default">
          <Image id="irImageIrScreen" width="88" height="66" url="{this.thumbnailUrl}"/>
          <VBox id="upNextBorder" width="88" height="66" visible="{this.isOver}" styleName="ThumbSelected"/>
        </Canvas>
        <VBox id="labelsHolder" height="66" width="100%" styleName="List_background_default">
          <Canvas id="irLinkIrScreen" height="18" width="100%">
            <Label id="nameLabel" height="18" width="100%" text="{this.name}" visible="{not(this.isOver)}" styleName="itemRendererTitleLabel_Up"/>
            <Label id="hoverNameLabel" height="18" width="100%" text="{this.name}" visible="{this.isOver}" styleName="itemRendererTitleLabel_Selected"/>
          </Canvas>
          <Canvas id="irDescriptionIrScreen" height="18" width="100%">
            <Text id="descriptionLabel" height="100%" width="100%" text="{this.description}" visible="{not(this.isOver)}" styleName="itemRendererLabel_Up"/>
            <Text id="hoverDescriptionLabel" height="100%" width="100%" text="{this.description}" visible="{this.isOver}" styleName="itemRendererLabel_Selected"/>
          </Canvas>
          <Canvas id="irDurationIrScreen" height="18" width="70">
            <Label id="durationLabel" height="18" width="70" text="{formatDate(this.duration, 'NN:SS')}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverDurationLabel" height="18" width="70" text="{formatDate(this.duration, 'NN:SS')}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irPlaysIrScreen" height="18" width="100%">
            <Label id="playsLabel" width="100%" height="18" text="Plays: {this.plays}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverPlaysLabel" width="100%" height="18" text="Plays: {this.plays}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irRankIrScreen" height="18" width="100%">
            <Label id="rankLabel" width="100%" height="18" text="Rank: {this.rank}}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverRankLabel" width="100%" height="18" text="Rank: {this.rank}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irVotesIrScreen" height="18" width="100%">
            <Label id="votesLabel" width="100%" height="18" text="{this.votes} votes" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverVotesLabel" width="100%" height="18" text="{this.votes} votes" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irTagsIrScreen" height="18" width="100%">
            <Label id="tagsLabel" width="100%" height="18" text="Tags: {this.tags}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverTagsLabel" width="100%" height="18" text="Tags: {this.tags}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irAdminTagsIrScreen" height="18" width="100%">
            <Label id="adminTagsLabel" width="100%" height="18" text="Categories: {this.categories}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverAdminTagsLabel" width="100%" height="18" text="Categories: {this.categories}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irCreatedAtIrScreen" height="18" width="100%">
            <Label id="createdAtLabel" width="100%" height="18" text="Created at: {formatDate(this.createdAt, 'dd/mm/yyy')}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverCreatedAtLabel" width="100%" height="18" text="Created at: {formatDate(this.createdAt, 'dd/mm/yyy')}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
          <Canvas id="irCreatedByIrScreen" height="18" width="100%">
            <Label id="createdByLabel" width="100%" height="18" text="Created by: {this.userScreenName}" styleName="itemRendererLabel_Up" visible="{not(this.isOver)}"/>
            <Label id="hoverCreatedByLabel" width="100%" height="18" text="Created by: {this.userScreenName}" styleName="itemRendererLabel_Selected" visible="{this.isOver}"/>
          </Canvas>
        </VBox>
        <VBox id="arrowVBox" height="100%" width="20" verticalAlign="middle">
          <Button id="irArroBtn" icon="RightArrow_Hover" visible="{this.isOver}"/>
        </VBox>
      </HBox>
    </renderer>
