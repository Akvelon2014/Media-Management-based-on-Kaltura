          <Button id="uploadBtnStartScreen" kClick="jsCall('gotoContributorWindow', mediaProxy.entry.id )" label="Upload" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_uploadIcon_up" upIcon="OnVideo_uploadIcon_up" overIcon="OnVideo_uploadIcon_Hover" downIcon="OnVideo_uploadIcon_Down" disabeledIcon="OnVideo_uploadIcon_Disabled"/>
          <Button id="downloadBtnStartScreen" kClick="sendNotification('doDownload')" label="Download" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_downloadIcon_Up" upIcon="OnVideo_downloadIcon_Up" overIcon="OnVideo_downloadIcon_Hover" downIcon="OnVideo_downloadIcon_Down" disabeledIcon="OnVideo_downloadIcon_Disabled"/>
          <Button id="captureThumbBtnStartScreen" kClick="sendNotification('captureThumbnail')" label="Thumb" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_thumbIcon_Up" upIcon="OnVideo_thumbIcon_Up" overIcon="OnVideo_thumbIcon_Hover" downIcon="OnVideo_thumbIcon_Down" disabeledIcon="OnVideo_thumbIcon_Disabled"/>
          <Button id="fullScreenBtnStartScreen" command="fullScreen" label="Fullscreen" minWidth="80" minHeight="80" labelPlacement="bottom" textPadding="5" styleName="onScreenBtn" icon="OnVideo_closeFullScreenIcon_Up" upIcon="OnVideo_closeFullScreenIcon_Up" overIcon="OnVideo_closeFullScreenIcon_Hover" downIcon="OnVideo_closeFullScreenIcon_Down" disabeledIcon="OnVideo_closeFullScreenIcon_Disabled" selectedUpIcon="OnVideo_openFullScreenIcon_Up" selectedOverIcon="OnVideo_openFullScreenIcon_Hover" selectedDownIcon="OnVideo_openFullScreenIcon_Down" selectedDisabledIcon="OnVideo_openFullScreenIcon_Disabled"/>
